  align-items: flex-end;


/******** ESTILOS PARA LISTAS **********/
ul
  margin: 0
  padding: 0


/****** ESTILOS PARA HIDE Y SHOW ******/
& .hide
  display: none

@media all and (max-width: 1439px)
  & .hide-lg
    display: none
@media all and (max-width: 1023px)
  & .hide-md
    display: none
@media all and (max-width: 767px)
  & .hide-sm
    display: none

& .show
  display: block;

@media all and (min-width: 1024px)
  & .show-lg
    display: block
@media all and (min-width: 768px)
  & .show-md
    display: block
@media all and (min-width: 480px)
  & .show-sm
    display: block

@media all and (min-width: 1024px)
  & .show-only-lg
    display: block
@media all and (min-width: 768px) and (max-width: 1023px)
  & .show-only-md
    display: block
@media all and  (max-width: 768px)
  & .show-only-sm
    display: block
/******  FLOATS  **********/
.float-left
  float: left
.float-right
  float right;

/********** CURSOR *********/
.pointer
  cursor: pointer;


